<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Random Photo</title>
    <script>
        const photos = [
            "https://drive.google.com/uc?id=1o_fZ4jDTNS0tKTvaqkvBVVxHvZKNGCPz",
            "https://drive.google.com/uc?id=1gQUtxm57u9m2l82I9q122WwJyVF_DH3Y",
            "https://drive.google.com/uc?id=1m6SdjxvJsP17y4YAvCkNmt7iZn19ffw6"
        ];

        // Redirection vers une photo alÃ©atoire
        window.onload = () => {
            const randomIndex = Math.floor(Math.random() * photos.length);
            window.location.href = photos[randomIndex];
        };
    </script>
</head>
<body>
    <p>Redirecting to a random photo...</p>
</body>
</html>
